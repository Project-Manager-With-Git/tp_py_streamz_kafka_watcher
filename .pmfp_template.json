{
    "language": "py",
    "env": "venv",
    "description": "用于监听kafka消息的流应用模板,使用streamz,同时提供了用于测试的confluent-kafka的生产者应用",
    "author": "hsz12",
    "template_type": "watcher",
    "requires": [
        "pyproxypattern >= 0.0.1",
        "schema_entry >= 0.1.5",
        "pyloggerhelper >= 0.0.2",
        "confluent-kafka >= 1.6.1",
        "pandas >= 1.2.4",
        "streamz >= 0.6.2"
    ],
    "test_requires": [
        "coverage >= 5.5",
        "mypy >= 0.800",
        "autopep8 >= 1.5.6",
        "pylint >= 2.8.0",
        "pydocstyle>=6.0.0"
    ],
    "template_keys": {
        "app_name": {
            "description": "项目名",
            "default": "project_name"
        }
    },
    "components": {
        "tp_py_streamz_kafka_watcher_sender": {
            "source": "tp_py_streamz_kafka_watcher_sender",
            "description": "",
            "default_path": "tp_py_streamz_kafka_watcher_sender"
        },
        "tp_py_streamz_kafka_watcher_watcher": {
            "source": "tp_py_streamz_kafka_watcher_watcher",
            "description": "",
            "default_path": "tp_py_streamz_kafka_watcher_watcher"
        }
    }
}